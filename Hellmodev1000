	"https://raw.githubusercontent.com/localplayerr/Doors-stuff/refs/heads/main/Immersive%20mode/Detph"
}

--------------------------------------------------
-- KIỂM TRA CỬA (>=10)
--------------------------------------------------
local function CanSpawn()
	local stats = game.ReplicatedStorage:FindFirstChild("GameStats")
	if not stats then return false end

	local door = stats:FindFirstChild("LatestRoom")
	if not door then return false end

	return door.Value >= 10
end

--------------------------------------------------
-- LOGIC SỐ LƯỢNG SPAWN
--------------------------------------------------
local function GetSpawnCount()
	local r = math.random()

	if r <= 0.000000001 then
		return math.random(3, 5) -- siêu hiếm
	elseif r <= 0.005 then
		return 2 -- 0.5%
	else
		return 1 -- mặc định
	end
end

--------------------------------------------------
-- SPAWN ENTITY
--------------------------------------------------
task.spawn(function()
	while true do
		task.wait(math.random(60, 120))

		if not CanSpawn() then
			continue
		end

		local count = GetSpawnCount()

		for i = 1, count do
			task.spawn(function()
				local link = Entities[math.random(#Entities)]
				pcall(function()
					loadstring(game:HttpGet(link))()
				end)
			end)
			task.wait(0.6)
		end
	end
end)

--------------------------------------------------
-- CEASE ENTITY
--------------------------------------------------
task.spawn(function()
	while true do
		task.wait(math.random(180, 300))

		if not CanSpawn() then
			continue
		end

		pcall(function()
			loadstring(game:HttpGet(
				"https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/V2/Source.lua"
			))()
		end)
	end
end)
